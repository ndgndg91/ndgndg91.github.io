{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "react-snap",
    "build:github": "npm run build && cp dist/index.html dist/404.html && npm run generate:sitemap",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@mui/material": "^7.1.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "crypto-js": "^4.2.0",
    "jsencrypt": "^3.3.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.509.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^3.3.3",
    "three": "^0.176.0",
    "uuid": "^11.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@eslint/js": "^9.25.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "events": "^3.3.0",
    "globals": "^16.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.5.3",
    "process": "^0.11.10",
    "react-snap": "^1.23.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "terser": "^5.34.1",
    "timers-browserify": "^2.0.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "util": "^0.12.5",
    "vite": "^6.3.5"
  },
  "reactSnap": {
    "source": "dist",
    "destination": "dist",
    "crawlFrom": "/",
    "include": [
      "/",
      "/tools/encode-decode/base64",
      "/tools/encode-decode/url",
      "/tools/string/json-parser",
      "/tools/string/xml-parser",
      "/tools/string/uuid-generator",
      "/tools/string/random-hex",
      "/tools/string/string-diff-checker",
      "/tools/string/byte-counter",
      "/tools/string/html-escape-unescape",
      "/tools/string/regex-cheatsheet",
      "/tools/time/timestamp",
      "/tools/image/format-converter",
      "/tools/token/jwt",
      "/tools/hash/sha1",
      "/tools/hash/sha2",
      "/tools/hash/sha3",
      "/tools/encrypt-decrypt/aes",
      "/tools/encrypt-decrypt/rsa",
      "/tools/fun/roulette",
      "/tools/fun/color-palette",
      "/tools/fun/qr-generator",
      "/tools/network/ip-address",
      "/blog/software-engineer/list",
      "/blog/software-engineer/list/about-g1gc",
      "/blog/software-engineer/list/about-zgc",
      "/blog/software-engineer/list/cors-sop",
      "/blog/software-engineer/list/distributed-lock",
      "/blog/software-engineer/list/documentdb-cons",
      "/blog/software-engineer/list/etag",
      "/blog/software-engineer/list/handle-floating-point",
      "/blog/software-engineer/list/http-cookie",
      "/blog/software-engineer/list/jvm-warmup",
      "/blog/software-engineer/list/k8s-probes-spring-boot",
      "/blog/software-engineer/list/kafka-basic",
      "/blog/software-engineer/list/kafka-consumer-performance",
      "/blog/software-engineer/list/kafka-consumer-rate",
      "/blog/software-engineer/list/mcp-filesystem",
      "/blog/software-engineer/list/mongo-sharding",
      "/blog/software-engineer/list/r2dbc-conn-pool",
      "/blog/software-engineer/list/replay-attack",
      "/blog/software-engineer/list/request-context-holder",
      "/blog/software-engineer/list/simple-distributed-id-generation"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-dev-shm-usage"
    ],
    "minifyHtml": true,
    "inlineCss": true,
    "fixWebpackChunksIssue": false
  }
}
